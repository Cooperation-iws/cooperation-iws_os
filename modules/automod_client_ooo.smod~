#!/bin/sh

RMOD_ENGINE=1.0
RMOD_CATEGORY='Software'
RMOD_SUBCATEGORY='Client'
RMOD_NAME='Office client'
RMOD_AUTHOR='Oliv'
RMOD_VERSION=0.2
RMOD_DESCRIPTION='Office tools for client'
RMOD_RUN_IN_CHROOT=True
RMOD_UPDATE_URL='http://cooperation-iws.gensys-net.eu/update/modules/'
URL_FREE=$(cat /tmp/url_mirroir)

echo "I: installing liveusb installer"

aptitude install -y openoffice.org myspell-dictionary-* hunspell-dictionary-* openoffice.org-l10n-* openoffice.org-help-* openoffice.org-thesaurus-* openoffice.org-hyphenation-* unixodbc cupsys-bsd libsane libxrender1 libgl1 openclipart-openoffice.org msttcorefonts pstoedit imagemagick libpaper-utils flpsed zip unzip
